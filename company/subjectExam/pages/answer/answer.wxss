page {
    width: 100%;
    height: 100%;
}

.main-frame {
    width: 100%;
    height: 100%;
    overflow: hidden;
}

.container {
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    align-items: center;
}

.questions {
    width: 100%;
    height: 1100rpx;
}

.tab-container {
    width: 100%;
    border-top: 4rpx solid #3371b0;
}

.float-top-panel {
    position: absolute;
    width: 100%;
    height: 100%;
}